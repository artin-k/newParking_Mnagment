<Window x:Class="WpfTest.parkingStatus"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Parking Status" Height="400" Width="600">
    <Grid>
        <TextBlock Text="Parking Status" FontSize="20" FontWeight="Bold" HorizontalAlignment="Center" Margin="0,10"/>
        <UniformGrid x:Name="spotGrid" Columns="6" Rows="5" Margin="10,50,10,10"/>
    </Grid>
</Window>
